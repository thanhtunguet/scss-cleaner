{
  "name": "scss-cleaner",
  "version": "0.1.3",
  "private": false,
  "main": "./cli.js",
  "module": "./cli.js",
  "license": "MIT",
  "homepage": "https://thanhtunguet.info/scss-cleaner",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:cli:js": "tsc -b tsconfig.cli.json",
    "build:cli": "run-s build:cli:js cp-files",
    "postbuild:cli": "chmod a+x dist/cli.js",
    "watch:cli": "tsc -b tsconfig.cli.json -w",
    "cp-files": "cp README.md package.json dist/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "bin": {
    "scss-clean": "./cli.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "antd": "^4.4.1",
    "bootstrap": "^4.5.0",
    "codemirror": "^5.55.0",
    "react": "^16.13.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "scss-parser": "^1.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/bootstrap": "^4.5.0",
    "@types/codemirror": "^0.0.96",
    "@types/jest": "^24.0.0",
    "@types/node": "^14.0.19",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/scss-parser": "^1.0.0",
    "commander": "^5.1.0",
    "firebase-tools": "^8.10.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  }
}